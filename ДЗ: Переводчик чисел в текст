try:
    number = int(input('Введите число от 1 до 5: ').strip())
except ValueError:
    print('Ошибка: Введены не корректные данные')
else:
    if number == 1:
        print('Соответствующее слово: One')
    elif number == 2:
        print('Соответствующее слово: Two')
    elif number == 3:
        print('Соответствующее слово: Three')
    elif number == 4:
        print('Соответствующее слово: Four')
    elif number == 5:
        print('Соответствующее слово: Five')
    else:
        print('Ошибка: Введены не корректные данные')
